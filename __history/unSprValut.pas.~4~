  public
    { Public declarations }
  end;

var
  fmSprValut: TfmSprValut;

implementation

{$R *.dfm}

{ TfmSprValut }

procedure TfmSprValut.fillForm;
begin
  oqSprValut.SQL.Clear;
   if not dmOra.OraSession.Connected then
     dmOra.OraSession.Connected := true;

end;

end.
